{
	"id": "nodebb-plugin-sso-ifsta",
	"name": "NodeBB IFSTA OAuth2 SSO",
	"description": "NodeBB Plugin that allows users to login/register via the IFSTA OAuth2 provider.",
	"url": "https://github.com/beardon/nodebb-plugin-sso-ifsta",
	"library": "./library.js",
	"hooks": [
		{ "hook": "action:app.load", "method": "init" },
		{ "hook": "filter:auth.init", "method": "getStrategy" },
		{ "hook": "filter:admin.header.build", "method": "addMenuItem" }
	],
	"templates": "./templates",
	"minver": "0.4.0"
}
